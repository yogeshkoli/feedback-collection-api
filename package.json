    "@sendgrid/mail": "^6.4.0",
    "concurrently": "^5.0.2",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.13.15",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.11",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "path-parser": "^6.1.0",
    "stripe": "^7.15.0"
  }
}
